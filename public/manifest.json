{
    "manifest_version": 3,
    "name": "Chrome-Whisper",
    "version": "0.15",
    "description": "A Chrome extension for transcribing audio built with Vite",
    "icons": {
        "16": "icon-16x16.png",
        "32": "icon-32x32.png",
        "144": "icon-144x144.png"
      },
    "permissions": ["storage"],
    "action": {
        "default_icon": {
            "16": "icon-16x16.png",
            "32": "icon-32x32.png",
            "144": "icon-144x144.png"
          },
        "default_popup": "index.html"
    }
}

